# Festival Scheduler Configuration
# Copy this to .env.local and customize for each event

# Event Information
EVENT_NAME="Your Festival Name"
EVENT_DESCRIPTION="Your festival description"
EVENT_LOGO="/mac-logo.png"
EVENT_DOMAIN="your-festival.com"

# Google Sheets Integration
GOOGLE_SHEETS_ID="your-spreadsheet-id-here"
GOOGLE_APPS_SCRIPT_WEBHOOK="your-webhook-url-here"

# Theme Customization
PRIMARY_COLOR="#4a90e2"
SECONDARY_COLOR="#7b68ee"
ACCENT_COLOR="#ff6b6b"

# Features Toggle
ENABLE_BOOKING_SYSTEM=true
ENABLE_PHOTOSHOOT_BOOKING=true
ENABLE_PARTICIPANT_TRACKING=true

# Contact Information
CONTACT_EMAIL="info@yourfestival.com"
CONTACT_PHONE="+1234567890"

# Social Media
FACEBOOK_URL=""
INSTAGRAM_URL=""
WEBSITE_URL=""